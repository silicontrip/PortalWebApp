<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="net.silicontrip.ingress.persistence" transaction-type="JTA">
	<provider>org.eclipse.persistence.jpa.PersistanceProvider</provider>
    <jta-data-source>jdbc/IngressResource</jta-data-source>
    <class>net.silicontrip.ingress.CellMUEntity</class>
    <class>net.silicontrip.ingress.Links</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
        <property name="eclipselink.target-database" value="MySQL"/>
    </properties>
  </persistence-unit>
</persistence>
