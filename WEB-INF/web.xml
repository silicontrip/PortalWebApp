<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>Portals</display-name>  
  <distributable/>
<welcome-file-list>
<welcome-file>index.html</welcome-file>
</welcome-file-list>
  <servlet>
	<servlet-name>PortalServlet</servlet-name>
	<servlet-class>net.silicontrip.ingress.PortalServlet</servlet-class>
	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
	<servlet-name>EntityServlet</servlet-name>
	<servlet-class>net.silicontrip.ingress.EntityServlet</servlet-class>
	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
	<servlet-name>LinkServlet</servlet-name>
	<servlet-class>net.silicontrip.ingress.LinkServlet</servlet-class>
	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
	<servlet-name>FieldServlet</servlet-name>
	<servlet-class>net.silicontrip.ingress.FieldServlet</servlet-class>
	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>PortalServlet</servlet-name>
    <url-pattern>/getPortals</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EntityServlet</servlet-name>
    <url-pattern>/submitEntity</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LinkServlet</servlet-name>
    <url-pattern>/getLinks</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FieldServlet</servlet-name>
    <url-pattern>/getMU</url-pattern>
  </servlet-mapping>
</web-app>
